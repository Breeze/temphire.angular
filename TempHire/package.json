  "description": "A reference architecture for data driven business apps using Angular and BreezeJs",
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "metadata": ".\\tools\\bin\\MetadataGenerator.exe -i ..\\DomainModel\\bin\\Debug\\DomainModel.dll -o metadata.json",
    "entities": "node ./tools/tsgen.js",
    "metadata-entities": "npm run metadata && npm run entities",
    "build:dev": "webpack --config webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
